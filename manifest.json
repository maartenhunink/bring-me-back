{
  "manifest_version": 2,
  "name": "Bring Me Back",
  "description": "This extension will bring you back to where you actually want to be by offering you some love and insight when you feel like you need your internet fix.",
  "version": "0.1",
  "incognito": "split",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [ "tabs", "webRequest", "webRequestBlocking", "\u003Call_urls>", "storage", "contextMenus", "identity"],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "scripts": ["js/db.js", "js/global.js", "js/background.js"]
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}